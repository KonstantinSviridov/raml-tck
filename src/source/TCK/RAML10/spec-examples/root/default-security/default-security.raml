#%RAML 1.0

# Objective: This RAML illustrates an example of how default media types get automatically applied or in the /send resources overriden.
# Expected result: no error message

# tags: default security, security, security scheme

title: Dropbox API
version: 1
baseUri: https://api.dropbox.com/{version}
securedBy: [ oauth_2_0, oauth_1_0 ]
securitySchemes:
  oauth_2_0: !include securitySchemes/oauth_2_0.raml
  oauth_1_0: !include securitySchemes/oauth_1_0.raml
